import yaml
from pathlib import Path

with open("data/suburbs.yaml") as f:
    suburbs = yaml.safe_load(f)

output_dir = Path("content/suburb")
output_dir.mkdir(parents=True, exist_ok=True)

template = """---
title: "Airport Transfer from {name} to Melbourne Airport"
slug: "{slug}"
name: "{name}"
layout: "suburb"
description: "Book your private airport transfer from {name} to Melbourne Airport with local, trusted drivers."
keywords: ["{name} airport transfer", "{name} to Melbourne Airport", "private taxi from {name}"]
---

"""

for s in suburbs:
    md = template.format(**s)
    with open(output_dir / f"{s['slug']}.md", "w") as f:
        f.write(md)
